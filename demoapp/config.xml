<?xml version='1.0' encoding='utf-8'?>
<widget id="com.microsoft.appcenter.cordova.demoapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="APPCENTER_ANALYTICS_ENABLE_IN_JS" value="false" />
    <preference name="APPCENTER_CRASHES_ALWAYS_SEND" value="false" />
    <platform name="android">
        <preference name="APP_SECRET" value="adabe47b-4deb-4be2-803b-d8db7eab3530" />
        <preference name="LOG_LEVEL" value="2" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <preference name="deployment-target" value="9.0" />
        <preference name="LOG_LEVEL" value="2" />
        <preference name="APP_SECRET" value="a123b578-dd08-46b1-9590-5a7ca0c3ab0d" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-file" spec="^4.0.0" />
    <plugin name="cordova-plugin-camera" spec="^3.0.0" />
    <plugin name="cordova-plugin-appcenter-analytics" spec="^0.5.0" />
    <plugin name="cordova-plugin-appcenter-crashes" spec="^0.5.0" />
    <plugin name="cordova-plugin-appcenter-push" spec="^0.5.0" />
    <plugin name="cordova-plugin-appcenter-shared" spec="^0.5.0" />
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="5.0.0" />
    <plugin name="cordova-plugin-generate-low-memory" spec="file:../cordova-plugin-generate-low-memory" />
</widget>
